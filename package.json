{
  "name": "redux-tower",
  "version": "0.0.5",
  "description": "Saga powered routing engine for Redux app.",
  "homepage": "https://github.com/kuy/redux-tower",
  "authors": [
    "Yuki Kodama <endflow.net@gmail.com>"
  ],
  "keywords": [
    "redux",
    "react",
    "router",
    "routing",
    "redux-saga",
    "saga"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kuy/redux-tower.git"
  },
  "main": "./lib/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --progress --content-base examples/",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/babel src --out-dir lib --ignore __tests__",
    "build:examples": "cross-env NODE_ENV=production ./node_modules/.bin/webpack",
    "test": "cross-env NODE_ENV=test nyc ava",
    "flow": "./node_modules/.bin/flow",
    "eslint": "eslint src/ examples/",
    "clean": "./node_modules/.bin/rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "dependencies": {
    "domain-specific-saga": "^0.0.1",
    "history": "^4.5.1",
    "querystring": "^0.2.0",
    "ruta3": "^2.0.1"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-plugin-unassert": "^2.1.2",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "coveralls": "^2.11.16",
    "cross-env": "^3.2.3",
    "css-loader": "^0.27.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "^0.10.0",
    "flow-bin": "^0.41.0",
    "nyc": "^10.1.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.0",
    "redux-logger": "^2.8.1",
    "redux-saga": "^0.14.3",
    "rimraf": "^2.5.4",
    "semantic-ui-css": "^2.2.4",
    "semantic-ui-react": "^0.67.0",
    "style-loader": "^0.13.1",
    "testdouble": "^1.11.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
